
/* 日历主要颜色相关样式 */

.default_color,
.default_weekend-color,
.default_handle-color,
.default_week-color {
    color: #448088;
}

.default_today {
    color: #fff;
    background-color: #874fb4;
}

.default_choosed {
    color: #fff;
    background-color: #448088;
}

.default_date-disable {
    color: #ddd9d9;
    background-color: #fff;
}
.default_normal-date {
    color: #000;
    /* background-color: #448088; */
}

.default_choosed.default_date-disable {
    color: #e2e2e2;
    background-color: #c2afb6;
}

.default_prev-month-date,
.default_next-month-date {
    color: #fff;
    
}


.default_date-desc-disable {
    color: #e2e2e2;
    
}


.default_todo-circle {
    border-color: #88d2ac;
    
}

.default_todo-dot {
    background-color: #448088;
}

.default_date-desc {
    color: #c2c2c2;
    
}

.default_date-desc-lunar {
    color: #e54d42;
}


.default_festival {
    color: #c2c2c2;
    
}
