/* pages/User/User.wxss */
.head{
  background-color: #f5f9fa;
  width: 750rpx;
  height: 26vh;
  position: relative;
}
.like{
  border: 3rpx solid #a2c5c7;
  height: 5.4vh;
  width: 100%;
  display: flex;
  flex-direction: row;
}
.myMessage,.myLikeHappyThing,.myHappyThing,.customAndBackup,.helpAndFeekback,.about{
  border: 3rpx solid #e9f0f1;
  height: 9vh;
  width: 100%;
  display: flex;
  flex-direction: row;

}
.icon{
  /* height: 5vh; */
  flex: 1;
  
}
.iconDetail{
  height: 5vh;
  width: 5vh;
  margin-left: 30rpx;
  margin-right: 10rpx;
  margin-top: 2vh;
}
.title{
  flex: 6;
  font:normal bolder 3vh "微软雅黑",arial,sans-serif;
  color: #448088;
  line-height: 9vh;
}
.bracket{
  flex: 1.3;
  font:normal bolder 3vh "微软雅黑",arial,sans-serif;
  color: #448088;
  line-height: 9vh;
}
.like_num{
  flex: 1.3;
  font:normal bolder 3vh "微软雅黑",arial,sans-serif;
  color: #448088;
  line-height: 9vh;
}


/* head begin */
.loginButton{
  margin: 0 auto;
  margin-top:5vh;
  width: 300rpx;
  height: 10vh;
  border:5rpx solid #448088;
  font:normal bolder 4vh "微软雅黑",arial,sans-serif;
  color: #448088;
  line-height: 10vh;
  text-align: center;
  border-radius: 10%;
}
.userInf{
  margin-top:2vh;
  margin-left: 60rpx;
  display: flex;
  flex-direction: row;
}
.profile{
  margin-left: 60rpx;
  font:normal bolder 3vh "微软雅黑",arial,sans-serif;
  color: #448088;
}


/* 用户数据 begin */
.userData{
  display: flex;
  flex-direction: row;
  width: 750rpx;
  height: 11vh;
  bottom: 0;
  position: absolute;
  /* margin-bottom:0; */
}
.continueLogin, .accumulativeRecord, .receiveLike{
  flex: 1;
  /* border: 5rpx solid #000000; */
  height: 100%;

}
.number{
  margin: 0 auto;
  display: flex;
  justify-content: center;
  font:normal normal 3vh "微软雅黑",arial,sans-serif;
  color: #448088;
  margin-top: 1vh;

}
.descriptionWord{
  margin: 0 auto;
  display: flex;
  justify-content: center;
  font:normal normal 27rpx "微软雅黑",arial,sans-serif;
  color: #448088;
  margin-top: 5rpx;
}

/* 用户数据 end */
.NAJIImage{
  border-radius: 50%;
  width: 11vh;
  height: 11vh;
}

/* head end */